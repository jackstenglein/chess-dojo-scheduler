{
    "name": "frontend",
    "version": "0.1.0",
    "private": true,
    "type": "module",
    "scripts": {
        "start": "next start",
        "start:build": "next start",
        "start:test": "npm run build:test && npm run start:build",
        "preinstall": "npm run touch-env && cd ../common && npm install",
        "touch-env": "touch .env.local .env.development.local .env.production.local .env.beta.local .env.test.local .env",
        "dev": "$npm_package_config_run_dev -- next dev --webpack",
        "precheck": "npm run lint && next build --webpack && npm run test:unit",
        "precheck:full": "npm run precheck && npm run test:playwright",
        "dev:precheck": "npm run precheck && npm run dev",
        "build": "npm run build:production",
        "build:production": "$npm_package_config_run_prod -- next build --webpack",
        "build:beta": "$npm_package_config_run_beta -- next build --webpack",
        "build:test": "$npm_package_config_run_test -- next build --webpack",
        "build:development": "$npm_package_config_run_dev -- next build --webpack",
        "test": "npm run test:unit && npm run test:playwright",
        "test:unit": "$npm_package_config_run_test -- vitest run",
        "test:playwright": "playwright test --config=playwright/playwright.config.ts",
        "test:playwright:ui": "playwright test --config=playwright/playwright.config.ts --ui",
        "lint": "npm run lint:types && eslint . --quiet",
        "lint:fix": "eslint --fix .",
        "lint:types": "tsc --noEmit",
        "lint:types:watch": "tsc --noEmit --watch",
        "format": "prettier src playwright --write",
        "format:check": "prettier src playwright --check",
        "docs": "npm run docs:build && npm run docs:serve",
        "docs:serve": "serve -l 8080 docs",
        "docs:build": "typedoc"
    },
    "config": {
        "run_dev": "dotenvx run --env-file=.env.development.local --env-file=.env.local --env-file=.env.development --env-file=.env",
        "run_test": "dotenvx run --env-file=.env.test.local --env-file=.env.local --env-file=.env.test --env-file=.env",
        "run_prod": "dotenvx run --env-file=.env.production.local --env-file=.env.local --env-file=.env.production --env-file=.env",
        "run_beta": "dotenvx run --env-file=.env.beta.local --env-file=.env.local --env-file=.env.beta --env-file=.env"
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    },
    "dependencies": {
        "@aws-amplify/adapter-nextjs": "^1.7.1",
        "@dotenvx/dotenvx": "^1.52.0",
        "@emotion/cache": "^11.13.5",
        "@emotion/react": "^11.14.0",
        "@emotion/styled": "^11.14.1",
        "@fortawesome/free-brands-svg-icons": "^6.7.2",
        "@fortawesome/free-solid-svg-icons": "^6.7.2",
        "@jackstenglein/chess": "^2.2.17",
        "@jackstenglein/chess-dojo-common": "file:../common",
        "@jackstenglein/react-scheduler": "^4.0.3",
        "@mui/icons-material": "^7.3.7",
        "@mui/lab": "^7.0.1-beta.21",
        "@mui/material": "^7.3.7",
        "@mui/material-nextjs": "^7.3.7",
        "@mui/x-charts": "^8.26.0",
        "@mui/x-data-grid-pro": "^8.26.0",
        "@mui/x-date-pickers-pro": "^8.26.0",
        "@savi2w/chess-tcn": "^1.0.0",
        "@types/react": "19.2.10",
        "@types/react-dom": "19.2.3",
        "async-mutex": "^0.5.0",
        "aws-amplify": "^6.15.7",
        "axios": "^1.13.3",
        "bad-words": "^4.0.0",
        "chessground": "^9.2.1",
        "comlink": "^4.4.2",
        "copy-to-clipboard": "^3.3.3",
        "deep-equal": "^2.2.3",
        "lodash.debounce": "^4.0.8",
        "luxon": "^3.7.2",
        "modern-screenshot": "^4.6.8",
        "moment": "^2.30.1",
        "next": "16.1.6",
        "next-navigation-guard": "^0.2.0",
        "react": "19.2.4",
        "react-activity-calendar": "^2.7.14",
        "react-charts": "^3.0.0-beta.57",
        "react-countdown-circle-timer": "^3.2.1",
        "react-dom": "19.2.4",
        "react-ga4": "^2.1.0",
        "react-google-button": "^0.8.0",
        "react-icons": "^5.5.0",
        "react-joyride-react19-compat": "^2.9.5-react19-compat",
        "react-markdown": "^10.1.0",
        "react-minimal-pie-chart": "^9.1.2",
        "react-resizable": "^3.1.3",
        "react-tooltip": "^4.4.3",
        "remark-gfm": "^4.0.1",
        "rrule": "^2.8.1",
        "sharp": "^0.34.5",
        "use-long-press": "^3.3.0",
        "usehooks-ts": "^3.1.1",
        "uuid": "^13.0.0",
        "zod": "^4.3.6"
    },
    "devDependencies": {
        "@eslint/js": "^9.39.2",
        "@istanbuljs/nyc-config-typescript": "^1.0.2",
        "@next/eslint-plugin-next": "16.1.6",
        "@playwright/test": "^1.58.0",
        "@testing-library/jest-dom": "^6.9.1",
        "@testing-library/react": "^16.3.2",
        "@types/deep-equal": "^1.0.4",
        "@types/lodash.debounce": "^4.0.9",
        "@types/luxon": "^3.7.1",
        "@types/node": "^25.3.0",
        "@types/react-copy-to-clipboard": "^5.0.7",
        "@types/react-resizable": "^3.0.8",
        "@types/uuid": "^10.0.0",
        "@vitejs/plugin-react": "^5.1.2",
        "command-line-args": "^6.0.1",
        "command-line-usage": "^7.0.3",
        "dotenv": "^17.2.3",
        "eslint": "^9.39.2",
        "eslint-config-next": "16.1.6",
        "eslint-plugin-react": "^7.37.5",
        "eslint-plugin-react-hooks": "^7.0.1",
        "globals": "^17.1.0",
        "happy-dom": "^19.0.2",
        "jiti": "^2.6.1",
        "nyc": "^17.1.0",
        "prettier": "^3.8.1",
        "prettier-plugin-organize-imports": "^4.3.0",
        "serve": "^14.2.5",
        "typedoc": "^0.28.16",
        "typescript": "^5.9.3",
        "typescript-eslint": "^8.54.0",
        "vite": "^7.1.9",
        "vitest": "^4.0.18",
        "zod-validation-error": "^5.0.0"
    },
    "overrides": {
        "d3-interpolate": "^3.0.1",
        "d3-color": "^3.1.0",
        "d3-scale": "^4.0.2",
        "@types/react": "19.2.10",
        "@types/react-dom": "19.2.3",
        "next-navigation-guard": {
            "next": "^16.0.0"
        }
    }
}
