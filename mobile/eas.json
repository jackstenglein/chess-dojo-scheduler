{
    "cli": {
        "version": ">= 3.8.1"
    },
    "build": {
        "development": {
            "developmentClient": true,
            "distribution": "internal",
            "ios": {
                "resourceClass": "m-medium"
            },
            "env": {
                "APP_VARIANT": "development"
            }
        },
        "ios-simulator": {
            "extends": "development",
            "ios": {
                "simulator": true
            }
        },
        "preview": {
            "distribution": "internal",
            "ios": {
                "resourceClass": "m-medium"
            },
            "env": {
                "APP_VARIANT": "preview"
            }
        },
        "production": {
            "ios": {
                "resourceClass": "m-medium"
            }
        }
    },
    "submit": {
        "production": {}
    }
}
